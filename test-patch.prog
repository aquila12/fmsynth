edit
  clr 1     ; Allocate 1 patch
  sel 0 32  ; Select patch 0, 32 slots (polyphony)
  pat 2 2 7 ; Patch def 2 ops, 2 lfo, 7 instructions
    lfo 0 5.0
    lfo 1 4.0
    op.r 0 2.0 ; Operator 0 relative frequency at 2.0*f
    op.r 1 1.0 ; Operator 1 relative frequency at 1.0*f
    op.e 1 20.0 10.0 0.2 0.7 3.0 ; Operator 1 envelope
    prog
      lmix 0 0.006
      feed 0
      mix 0 0.8
      feed 1
      mix 1 1.0
      amod 1 0.2
      out
    end
  end
end
run 24000
